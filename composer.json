{
    "name": "galatanovidiu/laravel-snacks",
    "description": ":Just some laravel snacks for my personal use",
    "license": "MIT",
    "authors": [
        {
            "name": "Galatan Ovidiu Iulian",
            "email": "galatanovidiu@gmail.com",
            "homepage": "https://rogio.com"
        }
    ],
    "homepage": "https://github.com/galatanovidiu/laravel-snacks",
    "keywords": ["Laravel", "LaravelSnacks"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Galatanovidiu\\LaravelSnacks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Galatanovidiu\\LaravelSnacks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Galatanovidiu\\LaravelSnacks\\LaravelSnacksServiceProvider"
            ],
            "aliases": {
                "LaravelSnacks": "Galatanovidiu\\LaravelSnacks\\Facades\\LaravelSnacks"
            }
        }
    }
}
